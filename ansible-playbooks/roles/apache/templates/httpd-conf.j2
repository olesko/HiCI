<VirtualHost *:80>
        ServerAdmin webmaster@localhost
        ServerName hici.com
        ServerAlias hici
        ProxyRequests Off
        <Proxy *>
                Order deny,allow
                Allow from all
        </Proxy>
        ProxyPreserveHost on
        ProxyPass / http://localhost:8080/ nocanon
        AllowEncodedSlashes NoDecode
</VirtualHost>